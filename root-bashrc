# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/var/task/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/var/task/etc/profile.d/conda.sh" ]; then
        . "/var/task/etc/profile.d/conda.sh"
    else
        export PATH="/var/task/bin:$PATH"
    fi
fi
unset __conda_setup
